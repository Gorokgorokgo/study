class Solution {
    public long solution(long n) {
        
        double answer = 0;

        for (int x = 1; x < n ; x++){
          if (x * x == n){
            answer = (x + 1) * (x + 1);
            break;
          } else {
            answer = -1;
          }
        }
        return (long) answer;
    }
}
