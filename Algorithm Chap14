class Solution {
    public int solution(int n) {
        int answer = 0;
                
        boolean check = 0 <= n && n <= 3000;
        
        if (check){
            for (int a = 1; a <= n; a++){
                if (n % a == 0){
                    answer += a;
                }
            }
        }
        
        return answer;
    }
}
