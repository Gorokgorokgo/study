class Solution {
    public String solution(String[] seoul) {
        String answer = "";
        int x = 0;
        int s_l = seoul.length;
        
        for (int i = 0; i < s_l; i++) {
            if (seoul[i].equalsIgnoreCase("kim")){
                x = i;
                break;
            }
        }
        answer = "김서방은 " + x + "에 있다";
        
        return answer;
    }
}
